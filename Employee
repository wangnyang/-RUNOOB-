    this.name = name;                                    //把name赋值给this.name
  }
  public viod empAge(int empAge){                        //定义一个公有的int类型的对象empAge
    age = empAge;                                        //把empAge赋值给age
  }
  public viod empDesignation(String empDesig){           //定义并赋值职位
  desination = empDesig;
  }
  public viod empSalary(double empSalary){
  salary = empSalary;                                     //定义并赋值薪水
  }
  public void printEmployee(){
    System.out.pringln("名字:"+name)                      //打印名字
    System.out.println("年龄"+age)                        //打印年龄
